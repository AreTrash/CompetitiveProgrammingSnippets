<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>lcmOnly</Title>
      <Shortcut>lcmOnly</Shortcut>
      <SnippetTypes>
        <SnippetType>Expansion</SnippetType>
      </SnippetTypes>
    </Header>
    <Snippet>
      <Declarations>
        <Literal>
          <ID>int</ID>
          <Default>int</Default>
        </Literal>
      </Declarations>
      <Code Language="csharp"><![CDATA[        public static $int$ Lcm($int$ x, $int$ y)
        {
            return x * (y / Gcd(x, y));
        }]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>